{
    "name":"bright-cloud-studio/modal-gallery",
    "description":"Create a gallery where each slide cam have unlimited hotspot links that trigger modal boxes.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "modal", "gallery", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^7.4",
        "contao/core-bundle": "^4.9",
	"menatwork/contao-multicolumnwizard-bundle": "^3.4.10",
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/modal_gallery/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/modal_gallery": "system/modules/modal_gallery"
            }
        }
    },
    "minimum-stability": "dev"
}
